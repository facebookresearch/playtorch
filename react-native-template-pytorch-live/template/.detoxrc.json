{
  "testRunner": "jest",
  "runnerConfig": "e2e/config.json",
  "skipLegacyWorkersInjection": true,
  "apps": {
    "ios.debug": {
      "type": "ios.app",
      "binaryPath": "ios/detox-build/Build/Products/Debug-iphonesimulator/PyTorchLiveExample.app",
      "build": "xcodebuild CC=clang CPLUSPLUS=clang++ LD=clang LDPLUSPLUS=clang++ -UseModernBuildSystem=YES -workspace ios/PyTorchLiveExample.xcworkspace -scheme PyTorchLiveExample -sdk iphonesimulator -configuration Debug -derivedDataPath ios/detox-build -quiet"
    },
    "ios.release": {
      "type": "ios.app",
      "binaryPath": "ios/detox-build/Build/Products/Debug-iphonesimulator/PyTorchLiveExample.app",
      "build": "xcodebuild CC=clang CPLUSPLUS=clang++ LD=clang LDPLUSPLUS=clang++ -UseModernBuildSystem=YES -workspace ios/PyTorchLiveExample.xcworkspace -scheme PyTorchLiveExample -sdk iphonesimulator -configuration Release -derivedDataPath ios/detox-build -quiet"
    },
    "android.debug": {
      "type": "android.apk",
      "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
      "build": "cd android && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug && cd .."
    },
    "android.release": {
      "type": "android.apk",
      "binaryPath": "android/app/build/outputs/apk/release/app-release.apk",
      "build": "cd android && ./gradlew assembleRelease assembleAndroidTest -DtestBuildType=release && cd .."
    }
  },
  "devices": {
    "simulator": {
      "type": "ios.simulator",
      "device": {
        "type": "iPhone 11"
      }
    },
    "emulator": {
      "type": "android.emulator",
      "device": {
        "avdName": "pytorch_live"
      }
    }
  },
  "configurations": {
    "ios.sim.debug": {
      "device": "simulator",
      "app": "ios.debug"
    },
    "ios.sim.release": {
      "device": "simulator",
      "app": "ios.release"
    },
    "android.emu.debug": {
      "device": "emulator",
      "app": "android.debug"
    },
    "android.emu.release": {
      "device": "emulator",
      "app": "android.release"
    }
  }
}
